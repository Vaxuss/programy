<Window x:Class="BankDesktop.CloseMonthWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BankDesktop"
        mc:Ignorable="d"
        Title="CloseMonthWindow" Height="200" Width="200">
    <StackPanel>
        <Label Content="Podaj Procent: "/>
        <TextBox x:Name="ProcentTextBox"/>
        <Label Content="Podaj kwotę: "/>
        <TextBox x:Name="KwotaTextBox" />
        <Button Content="Zatwierdż" Click="Button_Click"/>
    </StackPanel>
</Window>
